{
  "name": "bookfox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "start": "NODE_ENV='production' node server/index.js",
    "dev:app": "react-scripts start",
    "dev:server": "nodemon -x 'node -r dotenv/config' -w server server/index.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "express": "^4.16.2",
    "normalize.css": "^7.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^2.2.1"
  },
  "devDependencies": {
    "dotenv": "^4.0.0",
    "eslint-config-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4",
    "react-scripts": "1.0.14"
  },
  "proxy": "http://localhost:4000",
  "eslint": {
    "extends": [
      "prettier"
    ]
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "tabWidth": 4
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
